{
  "ConnectionStrings": {
    "DefaultConnection": "[used-for-dev-migrations]"
  },
  "PasswordOptions": {
    "RequiredLength": 12,
    "RequireNonAlphanumeric": true,
    "RequireDigit": true,
    "RequireLowercase": true,
    "RequireUppercase": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
